{
  "application": "cloudproxy",
  "jar": "cloudproxy.jar",

  "commands": [
    ["unzip","%{src_home}/zorroa-plugin-sdk/dist/lang-java-plugin.zip", "-d", "%{build_dir}/shared/plugins"],
    ["unzip","%{src_home}/zorroa-plugin-sdk/dist/zorroa-core-plugin.zip", "-d", "%{build_dir}/shared/plugins"],
    ["rm", "-rf", "%{build_dir}/shared/plugins/zorroa-core/models"],
    ["rm", "%{build_dir}/shared/plugins/zorroa-core/ffmpeg-3.0.2-1.2-macosx-x86_64.jar"],
    ["rm", "%{build_dir}/shared/plugins/zorroa-core/ffmpeg-3.0.2-1.2.jar"],
    ["rm", "%{build_dir}/shared/plugins/zorroa-core/ojdbc7.jar"],
    ["rm", "%{build_dir}/shared/plugins/zorroa-core/videoinput-0.200-1.2-macosx-x86_64.jar"],
    ["rm", "%{build_dir}/shared/plugins/zorroa-core/videoinput-0.200-1.2.jar"]
  ],
  "dirs": [
    "bin",
    "lib",
    "logs",
    "shared/plugins",
    "shared/ofs",
    "shared/jobs"
  ]
}
