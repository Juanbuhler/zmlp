{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": ["macros", "@emotion"],
  "env": {
    "test": {
      "presets": ["next/babel", "@emotion/babel-preset-css-prop"],
      "plugins": ["babel-plugin-dynamic-import-node"]
    }
  }
}
