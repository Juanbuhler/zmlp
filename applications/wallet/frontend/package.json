{
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "node -r esm devServer.js",
    "dev:mocked": "MOCKED=true node -r esm devServer.js",
    "dev:staging": "STAGING=true node -r esm devServer.js",
    "lint": "eslint '{pages,src}/**/*.js'",
    "prod": "next build && next start",
    "size": "rimraf .next && next build && bundlesize",
    "start": "next start",
    "test": "TZ=UTC jest --watch --notify --verbose --no-cache",
    "test:cover": "TZ=UTC jest --coverage --verbose --no-cache",
    "test:hook": "TZ=UTC jest --bail --findRelatedTests --no-cache"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 1 Safari version"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "proseWrap": "always"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/.*/__mocks__"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.svg$": "<rootDir>/__mocks__/svg.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "npm run test:hook"
    ]
  },
  "bundlesize": [
    {
      "//": "dependencies",
      "path": ".next/static/chunks/commons**.js",
      "maxSize": "8 kB"
    },
    {
      "//": "framework",
      "path": ".next/static/chunks/framework**.js",
      "maxSize": "42 kB"
    },
    {
      "//": "main runtime",
      "path": ".next/static/chunks/main**.js",
      "maxSize": "7 kB"
    },
    {
      "//": "polyfill",
      "path": ".next/static/chunks/polyfill**.js",
      "maxSize": "32 kB"
    },
    {
      "//": "webpack runtime",
      "path": ".next/static/chunks/webpack**.js",
      "maxSize": "1 kB"
    },
    {
      "//": "chunks",
      "path": ".next/static/chunks/!(commons|framework|main|polyfill|webpack)*.js",
      "maxSize": "29 kB"
    },
    {
      "//": "app.js",
      "path": ".next/static/chunks/pages/_app**.js",
      "maxSize": "28 kB"
    },
    {
      "//": "error page",
      "path": ".next/static/chunks/pages/_error**.js",
      "maxSize": "2 kB"
    },
    {
      "//": "all pages",
      "path": ".next/static/chunks/pages/**/!(_app|_error)*.js",
      "maxSize": "44 kB"
    }
  ],
  "dependencies": {
    "@emotion/babel-preset-css-prop": "11.0.0",
    "@emotion/react": "11.1.4",
    "@reach/combobox": "0.12.1",
    "@reach/listbox": "0.12.1",
    "@reach/skip-nav": "0.12.1",
    "@sentry/browser": "6.0.3",
    "@svgr/webpack": "5.5.0",
    "body-scroll-lock": "3.1.5",
    "camelcase": "6.2.0",
    "deep-filter": "1.0.2",
    "dotenv": "8.2.0",
    "focus-visible": "5.2.0",
    "next": "10.0.6",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-aria-modal": "4.0.0",
    "react-compound-slider": "3.3.1",
    "react-dom": "17.0.1",
    "react-grid-layout": "1.2.0",
    "react-json-pretty": "2.2.0",
    "react-resizable": "2.0.0",
    "react-tippy": "1.4.0",
    "react-use-clipboard": "1.0.7",
    "react-virtualized-auto-sizer": "1.0.4",
    "react-window": "1.8.6",
    "react-window-infinite-loader": "1.0.5",
    "swr": "0.4.1",
    "use-resize-observer": "7.0.0",
    "utf8": "3.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.0.0",
    "@emotion/jest": "11.1.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-macros": "3.0.1",
    "bundlesize": "0.18.1",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "esm": "3.2.25",
    "express": "4.17.1",
    "http-proxy-middleware": "1.0.6",
    "husky": "4.3.8",
    "import-all.macro": "3.1.0",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "lint-staged": "10.5.3",
    "morgan": "1.10.0",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "webpack-bundle-analyzer": "4.4.0"
  }
}
