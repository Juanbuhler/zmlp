{
  "private": true,
  "engines": {
    "node": ">= 16",
    "npm": ">= 7"
  },
  "scripts": {
    "gui": "cypress open",
    "cli": "cypress run"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "proseWrap": "always"
  },
  "dependencies": {
    "cypress": "7.7.0"
  },
  "devDependencies": {
    "eslint": "7.31.0",
    "eslint-plugin-cypress": "2.11.3",
    "prettier": "2.3.2"
  }
}
