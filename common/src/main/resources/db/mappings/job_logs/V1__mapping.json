{
  "migration": {
    "reindex": true,
    "reingest": false
  },
  "settings": {
    "analysis": {
      "analyzer": {
        "default": {
          "tokenizer": "keyword",
          "filter": [
            "trim",
            "delimiter_filter",
            "lowercase"
          ]
        },
        "path_analyzer": {
          "type": "custom",
          "tokenizer": "path_tokenizer"
        },
        "class_analyzer": {
          "type": "custom",
          "tokenizer": "package_tokenizer"
        }
      },
      "filter": {
        "delimiter_filter": {
          "type": "word_delimiter",
          "preserve_original": "true"
        }
      },
      "tokenizer": {
        "path_tokenizer": {
          "type": "path_hierarchy",
          "delimiter": "/"
        },
        "package_tokenizer": {
          "type": "path_hierarchy",
          "delimiter": "."
        }
      }
    }
  },
  "mappings": {
    "entry": {
      "_all": {
        "enabled": false
      },
      "dynamic": true,
      "_timestamp": {
        "enabled": false
      },
      "properties": {
        "timestamp": {
          "type": "date"
        },
        "message": {
          "type": "string"
        },
        "assetId": {
          "index": "not_analyzed",
          "type": "string"
        },
        "path": {
          "type": "string",
          "analyzer": "path_analyzer"
        },
        "service": {
          "type": "string",
          "index": "not_analyzed"
        },
        "phase": {
          "type": "string",
          "index": "not_analyzed"
        },
        "processor": {
          "type": "string",
          "analyzer": "class_analyzer"
        },
        "skipped": {
          "type": "boolean"
        },
        "stackTrace": {
          "type": "object",
          "properties": {
            "file": {
              "type": "string"
            },
            "className": {
              "type": "string",
              "analyzer": "class_analyzer"
            },
            "method": {
              "type": "string",
              "index": "not_analyzed"
            },
            "line": {
              "type": "integer",
              "index": "not_analyzed"
            }
          }
        }
      }
    }
  }
}
